<!DOCTYPE html>
<html>
  <head>
    <title>All Bookings</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <table border="2px">
        
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>NumTickets</th>
            <th>Email</th>
            <th>Team</th>
            <th>Superhero</th>
            <th>Message</th>
        </tr>

        <% bookings.forEach( function(booking) { %>
    
            <tr>
              <td><%= booking._id %></td>
            	<td><%= booking.name %></td>
            	<td><%= booking.numTickets %></td>
              <td><%= booking.email %></td>
              <td><%= booking.team %></td>
              <td><%= booking.superhero %></td>
              <td><%= booking.message %></td>
            </tr>
        
        <% }); %>
     </table>
  </body>
</html> 